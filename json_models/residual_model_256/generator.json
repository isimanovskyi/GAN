{
    "name": "generator",
    "variables": {
            "activation": {
                "type": "LeakyReLU",
                "args": {
                    "negative_slope": 0.2
                }
            },
            "use_batch_norm": true,
            "kernel_size": [5,5],
            "res_kernel_size": [3,3]
    },

    "layers": [
        {
            "type": "Dense",
            "args": { "features": 8192 },
            "activation": "var/activation",
            "use_batch_norm": false
        },
        {
            "type": "Reshape",
            "args": { "shape": [ 512, 4, 4 ] }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 512,
                "kernel_size": [3, 3],
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 256,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 256,
                "kernel_size": "var/kernel_size",
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 256,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 128,
                "kernel_size": "var/kernel_size",
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 128,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 64,
                "kernel_size": "var/kernel_size",
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 64,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 32,
                "kernel_size": "var/kernel_size",
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 32,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2DTranspose",
            "args": {
                "channels": 16,
                "kernel_size": "var/kernel_size",
                "strides": [ 2, 2 ],
                "padding": "same",
                "output_padding": [ 1, 1 ]
            },
            "activation": "var/activation",
            "use_batch_norm": "var/use_batch_norm"
        },
        {
            "type": "Residual",
            "args": {
                "filters": 16,
                "kernel_size": "var/res_kernel_size",
                "activation": "var/activation",
                "use_batch_norm": "var/use_batch_norm"
            }
        },
        {
            "type": "Conv2D",
            "args": {
                "channels": 3,
                "kernel_size": "var/kernel_size",
                "strides": [ 1, 1 ],
                "padding": "same"
            },
            "use_batch_norm": false
        }
    ]
}
